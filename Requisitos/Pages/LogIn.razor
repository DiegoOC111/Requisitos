@page "/LogIn"

<div class="wrapper">
    <form class="form-signin">
        <h2 class="form-signin-heading">Iniciar Sesion</h2>
        <input type="text" class="form-control" name="username" placeholder="Correo Electronico" required="" autofocus="" />
        <input type="password" class="form-control" name="password" placeholder="Contraseña" required="" />
        
        <button class="btn btn-lg btn-primary btn-block" style="background-color: #1C252C;" type="submit">Iniciar</button>
    </form>
</div>